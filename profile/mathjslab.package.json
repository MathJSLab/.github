
{
  "name": "mathjslab-repositories",
  "version": "0.0.0",
  "description": "MathJSLab organization GitHub repositories.",
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "pkg:clone": "git clone https://github.com/MathJSLab/mathjslab.git",
    "pkg:wiki:clone": "git clone https://github.com/MathJSLab/mathjslab.wiki.git",
    "pkg:clean": "cd mathjslab && npm run clean",
    "pkg:clean:all": "cd mathjslab && npm run clean:all",
    "pkg:update": "cd mathjslab && npm run update",
    "pkg:git:add:commit:push": "cd mathjslab && npm run git:add:commit:push",
    "app:clone": "git clone https://github.com/MathJSLab/mathjslab-app.git",
    "app:wiki:clone": "git clone https://github.com/MathJSLab/mathjslab-app.wiki.git",
    "app:clean": "cd mathjslab-app && npm run clean",
    "app:clean:all": "cd mathjslab-app && npm run clean:all",
    "app:update": "cd mathjslab-app && npm run update",
    "app:git:add:commit:push": "cd mathjslab-app && npm run git:add:commit:push",
    "org:clone": "git clone https://github.com/MathJSLab/.github.git mathjslab-github",
    "org:wiki:clone": "git clone https://github.com/MathJSLab/.github.wiki.git mathjslab-github.wiki",
    "org:clean": "cd mathjslab-github && npm run clean",
    "org:clean:all": "cd mathjslab-github && npm run clean:all",
    "org:update": "cd mathjslab-github && npm run update",
    "org:git:add:commit:push": "cd mathjslab-github && npm run git:add:commit:push",
    "www:clone": "git clone https://github.com/MathJSLab/mathjslab-www.git",
    "www:wiki:clone": "echo \"Warning: there is no wiki repository associated with the mathjslab-www repository\"",
    "www:clean": "cd mathjslab-www && npm run clean",
    "www:clean:all": "cd mathjslab-www && npm run clean:all",
    "www:update": "cd mathjslab-www && npm run update",
    "www:git:add:commit:push": "cd mathjslab-www && npm run git:add:commit:push",
    "all:clone": "npm run pkg:clone && npm run app:clone && npm run org:clone && npm run www:clone",
    "all:wiki:clone": "npm run pkg:wiki:clone && npm run app:wiki:clone && npm run org:wiki:clone && npm run www:wiki:clone",
    "all:clone:all": "npm run all:clone && npm run all:wiki:clone",
    "all:clean": "npm run pkg:clean && npm run app:clean && npm run org:clean && npm run www:clean",
    "all:clean:all": "npm run pkg:clean:all && npm run app:clean:all && npm run org:clean:all && npm run www:clean:all",
    "all:update": "npm run pkg:update && npm run app:update && npm run org:update && npm run www:update"
  }
}
